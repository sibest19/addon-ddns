{
  "name": "Cloudflare DDNS updater",
  "version": "1.1.1.7",
  "slug": "ddns",
  "url": "https://github.com/sibest19/addon-ddns",
  "description": "Updates the public IP address to all A records on listed zones on Cloudflare",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "services",
  "boot": "auto",
  "options": {
    "zones": [
      {
        "api_key": null,
        "zone_id": null,
        "dns_records": []
      }
    ],
    "update": 300
  },
  "schema": {
    "zones": [
      {
        "api_key": "str",
        "zone_id": "str",
        "dns_records": [
          "str"
        ]
      }
    ],
    "update": "int"
  },
  "map": [
    "config"
  ]
}